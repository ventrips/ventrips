{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": false,
    "alwaysStrict": false,
    "skipLibCheck": true,
    "target": "es2017",
    "checkJs": false,
    "lib": [
      "esnext.asynciterable",
      "dom"
    ],
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
