    "@angular/platform-server": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ng-bootstrap/ng-bootstrap": "^5.0.0-rc.1",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "@types/faker": "^4.1.5",
    "@types/lodash": "^4.14.135",
    "bootstrap": "^4.3.1",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "firebase": "^6.2.4",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "ngx-quill": "^6.1.0",
    "ngx-spinner": "^7.1.4",
    "ngx-toastr": "^10.0.4",
