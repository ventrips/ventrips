{
  "name": "ventrips",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "build": "ng build",
    "build:install": "npm run build:ssr",
    "deploy": "npm run build:ssr && node movedist.js && cd functions && firebase deploy --only functions,hosting",
    "deploy:all": "npm run build:ssr && node movedist.js && cd functions && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ventrips:server:production"
  },
  "private": true,
  "dependencies": {
